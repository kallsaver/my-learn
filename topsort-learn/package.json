{
  "name": "toposort-learn",
  "version": "0.0.7",
  "description": "a easy and flexible plugin for webpack to injecting js and css into html",
  "main": "index.js",
  "scripts": {
    "build:cjs": "rollup -c build/config.js --environment TARGET:cjs",
    "build:esm": "rollup -c build/config.js --environment TARGET:esm",
    "build:umd": "rollup -c build/config.js --environment TARGET:umd",
    "build": "node build/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kallsave/easy-html-webpack-plugin.git"
  },
  "keywords": [
    "easy-webpack-html-plugin",
    "hash",
    "chunkPipe"
  ],
  "author": "kallsave",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kallsave/easy-html-webpack-plugin/issues"
  },
  "homepage": "https://github.com/kallsave/easy-html-webpack-plugin#readme",
  "dependencies": {
    "toposort": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "rollup": "^0.63.5",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-node-resolve": "^3.3.0"
  }
}
